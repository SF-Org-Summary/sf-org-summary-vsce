{
  "name": "sf-org-summary",
  "displayName": "sf-org-summary",
  "description": "",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "publisher": "sforgsummary",
  "repository": {
    "type": "git",
    "url": "https://github.com/SF-Org-Summary/sf-org-summary-vsce"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sf-org-summary.summaryrefresh",
        "title": "Refresh Org Summary Sidebar"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "orgsummary-view",
          "title": "SF Org Summary",
          "icon": "media/os-icon.svg"
        }
      ]
    },
    "views": {
      "orgsummary-view": [
        {
          "type": "webview",
          "id": "orgsummary",
          "name": "SF Org Summary",
          "icon": "media/os-icon.svg",
          "contextualTitle": "Org Summary"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "rollup -c && webpack --mode production",
    "watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^1.0.10",
    "@types/mocha": "^10.0.6",
    "@types/mz": "^2.7.8",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "concurrently": "^8.2.2",
    "css": "^3.0.0",
    "eslint": "^8.54.0",
    "mz": "^2.7.0",
    "node-sass": "6.0.1",
    "rollup": "2.30",
    "rollup-plugin-svelte": "6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "slice-ansi": "^7.1.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.1",
    "svelte-select": "^5.6.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "vscode-uri": "^3.0.8",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "sf-org-summary-core": "^1.1.0"
  }
}
